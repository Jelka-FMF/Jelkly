{
    "name": "jelkly",
    "description": "Blockly editor for creating and editing Jelka FMF patterns",
    "license": "MIT",
    "version": "0.0.0",
    "homepage": "https://github.com/Jelka-FMF/Jelkly",
    "repository": "https://github.com/Jelka-FMF/Jelkly",
    "author": "Programerski klub FMF",
    "scripts": {
        "clean": "npm exec -- pxt clean",
        "serve": "npm exec -- pxt serve",
        "build": "npm exec -- pxt buildtarget"
    },
    "files": [
        "README.md",
        "pxtarget.json",
        "targetconfig.json",
        "built/*.js",
        "built/*.json",
        "built/*.d.ts",
        "built/hexcache",
        "built/web",
        "sim/public",
        "docs/*.md",
        "docs/*/*.md",
        "docs/*/*/*.md",
        "docs/static/*.svg",
        "docs/static/*.png",
        "docs/static/icons/favicon.ico"
    ],
    "packageManager": "npm@10.8.1",
    "dependencies": {
        "pxt-common-packages": "^12.1.1",
        "pxt-core": "^11.2.19"
    },
    "devDependencies": {
        "@types/marked": "0.3.0",
        "@types/node": "8.10.66",
        "@types/react": "16.0.0",
        "@types/react-dom": "16.0.0",
        "@types/web-bluetooth": "0.0.20",
        "less": "^4.2.0",
        "pxt": "^0.5.1",
        "typescript": "^5.6.3"
    },
    "overrides": {
        "pxt-common-packages": "$pxt-common-packages",
        "pxt-core": "$pxt-core",
        "pxt": "$pxt"
    }
}
