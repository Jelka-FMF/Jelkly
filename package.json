{
    "name": "jelkly",
    "description": "Blockly editor for creating and editing Jelka FMF patterns",
    "license": "MIT",
    "version": "0.0.0",
    "homepage": "https://github.com/Jelka-FMF/Jelkly",
    "repository": "https://github.com/Jelka-FMF/Jelkly",
    "author": "Programerski klub FMF",
    "scripts": {
        "clean": "npm exec -- pxt clean",
        "serve": "npm exec -- pxt serve",
        "build": "npm exec -- pxt staticpkg"
    },
    "files": [
        "README.md",
        "pxtarget.json",
        "targetconfig.json",
        "built/*.js",
        "built/*.json",
        "built/*.d.ts",
        "built/hexcache",
        "built/web",
        "sim/public",
        "docs/*.md",
        "docs/*/*.md",
        "docs/*/*/*.md",
        "docs/static/*.svg",
        "docs/static/*.png",
        "docs/static/icons/favicon.ico"
    ],
    "packageManager": "npm@10.8.1",
    "dependencies": {
        "pxt-common-packages": "^13.1.7",
        "pxt-core": "^12.2.13",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/marked": "^0.3.0",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^18.3.7",
        "@types/web-bluetooth": "^0.0.21",
        "less": "^4.5.1",
        "pxt": "^0.5.1",
        "typescript": "^5.9.3"
    },
    "overrides": {
        "pxt-common-packages": "$pxt-common-packages",
        "pxt-core": "$pxt-core",
        "pxt": "$pxt"
    }
}
